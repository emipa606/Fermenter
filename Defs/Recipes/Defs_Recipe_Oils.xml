<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--  Corn -->
  <RecipeDef>
    <defName>Press_FRMT_CornOil</defName>
    <label>press corn oil</label>
    <description>Press 25 corn oil using oil press.</description>
    <jobString>Pressing corn oil.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>3000</workAmount>
    <targetCountAdjustment>25</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawCorn</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawCorn</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <FRMT_CornOil>25</FRMT_CornOil>
    </products>
    <recipeUsers>
      <li>FRMT_OilPress</li>
    </recipeUsers>
    <researchPrerequisite>FRMT_OilPlants</researchPrerequisite>
  </RecipeDef>
