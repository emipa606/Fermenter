<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Dubs Bad Hygiene</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>/Defs</xpath>
          <value>
            <Fermenter.FermentDef>
              <defName>Ferment_DBH_Biosolids</defName>
              <Product>Biosolids</Product>
              <Resource>FecalSludge</Resource>
              <SpoilProduct>FecalSludge</SpoilProduct>
              <FermentHours>96.0</FermentHours>
              <ResearchProject>SewageSludgeComposting</ResearchProject>
            </Fermenter.FermentDef>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>